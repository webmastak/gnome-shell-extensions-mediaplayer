#: ../src/prefs.js:33
msgid "Show the media player in the volume menu"
msgstr "Exibir o media player no menu de volume"

#: ../src/prefs.js:34
msgid "Allow to start the default media player"
msgstr "Permitir iniciar o media player padrão"

#: ../src/prefs.js:35
msgid ""
"Runs the default mediaplayer by clicking on the media player status icon."
msgstr ""
"Executa o media player padrão clicando no ícone de status do media player"

#: ../src/prefs.js:36
msgid "Show the media player volume slider"
msgstr "Exibir o controle de volume do media player"

#: ../src/prefs.js:37
msgid "Show the media player position slider"
msgstr "Exibir o controle de posição do media player"

#: ../src/prefs.js:38
msgid "Show media player playlists"
msgstr "Exibir playlists do media player"

#: ../src/prefs.js:39
msgid "Display song rating"
msgstr "Mostrar a avaliação da música"

#: ../src/prefs.js:40
msgid "Display the currently playing song's rating on a 0 to 5 scale"
msgstr "Mostra a avaliação da música que está tocando em uma escala de 0 a 5"

#: ../src/prefs.js:43
msgid "Album cover size"
msgstr "Tamanho da capa do álbum"

#: ../src/prefs.js:44
msgid "The size of the cover displayed in the menu. Default is 80px width."
msgstr "O tamanho da capa exibida no menu. Por padrão, 80 pixels de largura."

